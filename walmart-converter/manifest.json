{
  "manifest_version": 3,
  "name": "Walmart Link Converter",
  "version": "1.1",
  "description": "Automatically convert Walmart links to business links.",
  "permissions": ["clipboardWrite", "activeTab"],
  "host_permissions": ["*://www.walmart.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
